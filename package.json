{
  "name": "zhishuyun-data",
  "version": "0.1.0",
  "scripts": {
    "start": "cross-env NODE_ENV=$npm_config_env vite --host",
    "start:local": "npm run start --env=local",
    "start:test": "npm run start --env=test",
    "start:production": "npm run start --env=production",
    "build": "cross-env NODE_ENV=$npm_config_env vue-tsc --noEmit --skipLibCheck && vite build",
    "build:local": "npm run build --env=local",
    "build:test": "npm run build --env=test",
    "build:production": "npm run build --env=production",
    "serve": "vite preview",
    "lint": "npx eslint src --fix --ext .ts,.tsx,.vue,.js,.jsx",
    "prettier": "npx prettier --write .",
    "init:https": "npm run init:ca && npm run init:cert",
    "init:ca": "npx mkcert create-ca --validity 36500 --key certs/data.local.zhishuyun.com.ca.key --cert certs/data.local.zhishuyun.com.ca.crt",
    "init:cert": "npx mkcert create-cert --ca-key certs/data.local.zhishuyun.com.ca.key --ca-cert certs/data.local.zhishuyun.com.ca.crt --validity 36500 --key certs/data.local.zhishuyun.com.cert.key --cert certs/data.local.zhishuyun.com.cert.crt  --domains data.local.zhishuyun.com"
  },
  "dependencies": {
    "@codemirror/lang-javascript": "^6.1.2",
    "@codemirror/lang-json": "^6.0.1",
    "@codemirror/theme-one-dark": "^6.1.0",
    "@fortawesome/fontawesome-svg-core": "^6.2.1",
    "@fortawesome/free-brands-svg-icons": "^6.2.1",
    "@fortawesome/free-regular-svg-icons": "^6.2.1",
    "@fortawesome/free-solid-svg-icons": "^6.2.1",
    "@fortawesome/vue-fontawesome": "^3.0.2",
    "@types/swagger-ui": "^3.52.0",
    "@types/uuid": "^8.3.4",
    "axios": "^0.21.1",
    "codemirror": "^6.0.1",
    "dayjs": "^1.10.7",
    "element-plus": "^1.3.0-beta.10",
    "hls.js": "^1.1.5",
    "humps": "^2.0.1",
    "pdfvuer": "^2.0.1",
    "qrcode.vue": "3.3.0",
    "swagger-ui": "^4.15.5",
    "tcadapter": "^0.0.8",
    "typescript-cookie": "^1.0.3",
    "url-join": "^5.0.0",
    "uuid": "^8.3.2",
    "vue": "^3.2.6",
    "vue-codemirror": "^6.1.1",
    "vue-i18n": "^9.1.7",
    "vue-pdf-embed": "^1.1.4",
    "vue-plyr": "^7.0.0",
    "vue-router": "^4.0.11",
    "vuex": "^4.0.2",
    "vuex-persistedstate": "^4.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-replace": "^3.0.0",
    "@types/hls.js": "^1.0.0",
    "@types/humps": "^2.0.1",
    "@types/lodash": "^4.14.172",
    "@types/node": "^16.7.4",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "@vitejs/plugin-vue": "^2.3.0",
    "@vue/compiler-sfc": "^3.0.5",
    "autoprefixer": "^10.3.3",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^8.5.0",
    "husky": "^7.0.2",
    "mkcert": "^1.4.0",
    "postcss": "^8.3.6",
    "prettier": "^2.6.2",
    "rollup-plugin-string": "^3.0.0",
    "sass": "^1.38.1",
    "tailwindcss": "^2.2.8",
    "typescript": "^4.6.3",
    "vite": "^2.9.0",
    "vue-tsc": "^0.29.8"
  }
}
